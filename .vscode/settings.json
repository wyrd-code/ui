{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar",
  },
  "[typescript, javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  },
  "editor.snippetSuggestions": "top",
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.linkedEditing": true,
  "css.customData": [
    "./.vscode/css-data.json"
  ],
  "html.autoClosingTags": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "eslint.format.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.validate": [
    "vue",
    "json",
    "javascript",
    "typescript"
  ],
  "eslint.options": {
    "extensions": [
      ".js",
      ".vue",
      ".json",
      ".ts"
    ]
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/node_modules": true,
    "**/dist": false,
    "**/tsconfig.tsbuildinfo": true,
    "**/.vitepress/cache": true,
  },
  "git.inputValidation": "off",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#93e6fc",
    "activityBar.activeBorder": "#fa45d4",
    "activityBar.background": "#93e6fc",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#fa45d4",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#93e6fc",
    "statusBar.background": "#61dafb",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#2fcefa",
    "statusBarItem.remoteBackground": "#61dafb",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#61dafb",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#61dafb99",
    "titleBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99"
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.general.statusBar": "top three",
  "todo-tree.general.statusBarClickBehaviour": "cycle",
  "todo-tree.tree.showCountsInTree": true,
  "todo-tree.general.showIconsInsteadOfTagsInStatusBar": false,
  "todo-tree.general.showActivityBarBadge": true,
  "todo-tree.highlights.highlightDelay": 0,
  "todo-tree.filtering.useBuiltInExcludes": "file excludes",
  "todo-tree.filtering.excludeGlobs": ["CONTRIBUTING.md", "CHANGELOG.md"],
  "todo-tree.tree.labelFormat": "${after}",
  "todo-tree.highlights.customHighlight": {
    "PENDING": {
      "foreground": "#e9f5f9",
      "iconColour": "#e9f5f9",
    },
    "DONE": {
      "foreground": "#96be25",
      "iconColour": "#96be25",
    },
    "FIX": {
      "foreground": "#be2596",
      "iconColour": "#be2596",
    },
  },
  "todo-tree.general.tags": [
    "TODO",
    "REFACTOR",
    "BUG",
    "FIX",
    "FIXME",
    "- [ ]",
    "- [ ] Fix",
    "- [x]"
  ],
  "todo-tree.general.tagGroups": {
    "DONE": [
      "- [x]",
    ],
    "PENDING": [
      "TODO",
      "REFACTOR",
      "- [ ]",
    ],
    "FIX": [
      "- [ ] Fix",
      "BUG",
      "FIX",
      "FIXME",
    ],
  },
  "peacock.color": "#61dafb",
  "unocss.root": ""
}
